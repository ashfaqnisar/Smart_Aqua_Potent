<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ezerka.smartaquapotent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LogoActivity"
            android:theme="@style/NoAppBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="Login" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity
            android:name=".user_activities.UserMenuActivity"
            android:label="Menu" />
        <activity
            android:name=".user_activities.UserProfileSettingsActivity"
            android:label="Profile Settings" />
        <activity
            android:name=".user_activities.UserSettingsActivity"
            android:label="Settings" />
        <activity android:name=".staff_activities.StaffMenuActivity" />
        <activity android:name=".staff_activities.StaffProfileSettingsActivity" />
        <activity android:name=".staff_activities.StaffSettingsActivity" />
        <activity android:name=".admin_activities.AdminMenuActivity" />
        <activity android:name=".admin_activities.AdminProfileSettingsActivity" />
        <activity android:name=".admin_activities.AdminSettingsActivity"></activity>
    </application>

</manifest>